
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")


}

model Form{
  id          Int      @id @default(autoincrement())
  name        String   
  email       String   @unique
  phone       String
  course      String
  submittedAt DateTime @default(now())
}
